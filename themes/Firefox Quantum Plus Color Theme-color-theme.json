	"tokenColors": [
		{
			"scope": [
				"constant.language.boolean",
				"constant.language.json",
				"constant.language"
			],
			"settings": {
				"foreground": "#6B7ABB"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#5C6773"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#6B7ABB"
			}
		},
		{
			"scope": [
				"variable",
				"variable.other",
				"support.variable",
